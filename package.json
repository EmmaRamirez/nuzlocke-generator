{
    "name": "nuzlocke-generator",
    "version": "1.19.0",
    "description": "A tool for generating nuzlocke team pics from data",
    "main": "dist/bundle.js",
    "type": "module",
    "scripts": {
        "dev": "vite --host",
        "build:production": "vite build --mode production",
        "build": "vite build",
        "build:watch": "vite build --watch",
        "clean": "rimraf dist",
        "cy:open": "npx cypress open",
        "cy:run": "npx cypress run",
        "deploy": "npm run build && git push heroku master",
        "lint": "npx eslint",
        "lint:fix": "npx eslint --fix",
        "lint:errors": "npx eslint --quiet",
        "prebuild": "rm -rf ./dist",
        "precommit": "npm run lint",
        "pretest": "npm run lint",
        "serve:heroku": "npm run build:production && heroku local web",
        "serve:node": "npx tsx server.ts",
        "serve:http": "npx http-server@0.12.0 --cors ./dist",
        "serve:forever": "npx forever start -c 'npm run server:node' ./",
        "serve": "npm run dev",
        "start": "npm run serve",
        "start:ci": "npm start",
        "test": "vitest --coverage",
        "todos": "npx leasot 'src/**/*.tsx' 'src/**/*.ts'",
        "lh": "npx lighthouse http://localhost:8080 --output-path=./lighthouse/audit.html --save-assets --preset=desktop --quiet --chrome-flags=headless"
    },
